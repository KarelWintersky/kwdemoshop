.highslide-dimming {
	background: white;
	position: absolute;
	width: 100%;
	visibility: hidden;
}
.highslide-html {
  background-color: white;
	overflow:hidden;
}
.highslide-html-blur {
	overflow:hidden;
}
.highslide-html-content-header {
	height: 10px;
	padding: 2px;
}
.highslide-html-content {
  background: #EEEEEE;
	position: absolute;
	display: none;
	width: 250px;
	overflow:hidden;
	overflow-y:hidden;
}
.highslide-body {
	padding: 0 10px 10px 20px;
	overflow:hidden;
	overflow-y:hidden;
}

.highslide-body1 {
	padding: 0 10px 10px 10px;
	overflow:hidden;
	overflow-y:hidden;
	height:190px !important;
}
.highslide-body2 {
	padding: 0 10px 10px 10px;
	overflow:hidden;
	overflow-y:hidden;
	height:70px !important;
}
.highslide-display-block {
    display: block;
	overflow:hidden;
	overflow-y:hidden;
}
.highslide-display-none {
    display: none;
}
.highslide-move {
	border: 0;
	height: 10px;
	padding: 2px;
}

a.control {
	float: right;
	display: block;
	margin: 0 0px;
	font-size: 12px;
	font-weight: 900;
	text-decoration: none;
	text-transform: uppercase;
	color: #999 !important;
}
a.control:hover {
	color: black !important;
	cursor: pointer;
}
